<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/threecar_third_boxbgtop"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/third_boxbgtop"
        android:orientation="horizontal" >
        <ImageView
            android:id="@+id/newc_imv_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:layout_centerVertical="true"
            android:background="@drawable/title_arrow" />
        <TextView
            android:id="@+id/threecar_tv_threecarname"
            android:layout_toRightOf="@+id/newc_imv_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="三者车1"
            android:layout_marginLeft="10dp" />
        <ImageView
            android:id="@+id/threecar_imv_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/third_deleteicon" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/sunbjectcar_third_boxbgbottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sunbjectcar_third_boxbgtop"
        android:background="@drawable/third_boxbgbottom"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/threecar_tv_carno"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="号牌号码" />

            <EditText
                android:id="@+id/threecar_ed_carno"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:singleLine="true"
                android:layout_toRightOf="@+id/threecar_tv_carno"
                android:background="@drawable/input_bg" />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/threecar_tv_cartype"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="车辆种类" />

            <include
                android:id="@+id/threecar_spinner_insurance_cartype"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/threecar_tv_cartype"
                layout="@layout/common_spinner" />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />
        
        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/threecar_tv_chassis"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="车架号" />

            <EditText
                android:id="@+id/threecar_ed_chassis"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="10dp"
                android:layout_toRightOf="@+id/threecar_tv_chassis"
                android:text="11"
       	 		android:singleLine="true" 
                android:background="@drawable/input_bg" />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/threecar_tv_factory"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="厂牌型号" />

            <EditText
                android:id="@+id/threecar_ed_factory"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_toRightOf="@+id/threecar_tv_factory"
                android:text="11"
       	 		android:singleLine="true" 
                android:background="@drawable/input_bg" />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/threecar_tv_duty"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="责任比例" />

            <EditText
                android:id="@+id/threecar_ed_duty"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:inputType="numberSigned"
       	 		android:singleLine="true" 
                android:layout_toRightOf="@+id/threecar_tv_duty"
                android:background="@drawable/input_bg" />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/threecar_tv_policyno"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="保单号" />

            <EditText
                android:id="@+id/threecar_ed_policyno"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="10dp"
       	 		android:singleLine="true" 
                android:layout_toRightOf="@+id/threecar_tv_policyno"
                android:background="@drawable/input_bg" />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/threecar_tv_insurance_company"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="承保公司" />
            <include
                android:id="@+id/threecar_spinner_insurance_company"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/threecar_tv_insurance_company"
                layout="@layout/common_spinner" />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/threecar_tv_engineno"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="发动机号" />

            <EditText
                android:id="@+id/threecar_ed_engineno"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_toRightOf="@+id/threecar_tv_engineno"
                android:background="@drawable/input_bg"
                android:text="11"
       	 		android:singleLine="true"  />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <TextView
                android:id="@+id/threecar_tv_flappertype"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="号牌种类" />
            
		
            <include
                android:id="@+id/threecar_spinner_flappertype"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/threecar_tv_flappertype"
                layout="@layout/common_spinner" />
             
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />
        
        <!-- （浙江）驾驶员姓名 -->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" 
            android:id="@+id/rel_province_zhejiang_nulldrivername">

            <TextView
                android:id="@+id/threecar_tv_nulldrivername"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="驾驶员姓名" />

            <EditText
                android:id="@+id/threecar_ed_nulldrivername"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_toRightOf="@+id/threecar_tv_nulldrivername"
                android:background="@drawable/input_bg"
       	 		android:singleLine="true"  />
        </RelativeLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />
        <!-- （浙江）驾驶员证件类型 -->
            <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp" 
            android:id="@+id/rel_province_zhejiang_nullcertitype">
            <TextView
                android:id="@+id/threecar_tv_nullcertitype"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="驾驶员证件类型" />
            <include
                android:id="@+id/threecar_spinner_nullcertitype"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/threecar_tv_nullcertitype"
                layout="@layout/common_spinner" />
        </RelativeLayout>
        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />
        <!-- （浙江）驾驶员证件号码 -->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/rel_province_zhejiang_nulldrivercode" >
            <TextView
                android:id="@+id/threecar_tv_nulldrivercode"
                style="@style/word_list_item_textview_title_style"
                android:layout_marginTop="15dp"
                android:text="驾驶员证件号码" />
            <EditText
                android:id="@+id/threecar_ed_nulldrivercode"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_toRightOf="@+id/threecar_tv_nulldrivercode"
                android:background="@drawable/input_bg"
       	 		android:singleLine="true"  />
        </RelativeLayout>
        <View
            android:layout_width="wrap_content"
            android:layout_height="0.1dp"
            android:background="#66000000"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />
        
    </LinearLayout>

</LinearLayout>